{
    "name": "another-react-boilerplate",
    "private": true,
    "scripts": {
        "build": "npm run clean-dist && webpack --config=configs/webpack/prod && cp ../API/public/index.php ../dist/ && cp ../API/public/.htaccess ../dist/ && cp ../API/public/favicon.ico ../dist/",
        "build:dev": "npm run clean-dist && webpack --config=configs/webpack/dev && cp ../API/public/index.php ../dist/ && cp ../API/public/.htaccess ../dist/ && cp ../API/public/favicon.ico ../dist/",
        "clean-dist": "rm -f -r -d ../dist/*",
        "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss,ts,tsx}\"",
        "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss,ts,tsx}\"",
        "start": "npm run start-dev",
        "start-dev": "webpack serve --config=configs/webpack/dev.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.8",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/react-redux": "^7.1.23",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "babel-loader": "^8.2.4",
        "css-loader": "^6.7.1",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "express": "^4.17.3",
        "html-webpack-plugin": "^5.5.0",
        "image-webpack-loader": "^8.1.0",
        "jest": "^27.5.1",
        "node-sass": "^7.0.1",
        "prettier": "^2.6.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.6.0",
        "style-loader": "^3.3.1",
        "typescript": "^4.6.3",
        "url": "^0.11.0",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "axios": "^0.26.1",
        "bootstrap": "^5.1.3",
        "classnames": "^2.3.1",
        "copy-to-clipboard": "^3.3.1",
        "jquery": "^3.6.0",
        "localstoragedb": "^2.3.2",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "moment": "^2.29.2",
        "react-custom-scrollbars": "^4.2.1",
        "react-icons": "^4.3.1",
        "react-redux": "^7.2.6",
        "react-router": "^6.2.2",
        "react-router-dom": "^6.2.2",
        "react-transition-group": "^4.4.2",
        "reactstrap": "^9.0.1",
        "redux-auth-wrapper": "^3.0.0",
        "redux-form": "^8.3.8",
        "redux-thunk": "^2.4.1",
        "roboto-fontface": "^0.10.0",
        "typeface-spectral": "^1.1.13"
