{
    "name": "another-react-boilerplate",
    "private": true,
    "scripts": {
        "build": "npm run clean-dist && webpack --config=configs/webpack/prod && cp ../API/public/index.php ../dist/ && cp ../API/public/.htaccess ../dist/ && cp ../API/public/favicon.ico ../dist/",
        "build:dev": "npm run clean-dist && webpack --config=configs/webpack/dev && cp ../API/public/index.php ../dist/ && cp ../API/public/.htaccess ../dist/ && cp ../API/public/favicon.ico ../dist/",
        "clean-dist": "rm -f -r -d ../API/public/js/* && rm -f -r -d ../API/public/fonts/*",
        "prettier-write": "prettier --write \"{src,tests}/**/*.{js,jsx,css,scss,ts,tsx}\"",
        "prettier-check": "prettier --list-different \"{src,tests}/**/*.{js,jsx,css,scss,ts,tsx}\"",
        "start": "npm run start-dev",
        "start-dev": "webpack serve --config=configs/webpack/dev.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.7",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.17",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "babel-loader": "^9.1.0",
        "css-loader": "^6.7.3",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "express": "^4.18.2",
        "html-webpack-plugin": "^5.5.0",
        "image-webpack-loader": "^8.1.0",
        "jest": "^29.3.1",
        "node-sass": "^8.0.0",
        "prettier": "^2.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.1",
        "typescript": "^4.9.4",
        "url": "^0.11.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "axios": "^1.2.1",
        "bootstrap": "^5.2.3",
        "classnames": "^2.3.2",
        "copy-to-clipboard": "^3.3.3",
        "jquery": "^3.6.3",
        "localstoragedb": "^2.3.2",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "moment": "^2.29.4",
        "react-custom-scrollbars": "^4.2.1",
        "react-icons": "^4.7.1",
        "react-redux": "^8.0.5",
        "react-router": "^6.6.1",
        "react-router-dom": "^6.6.1",
        "react-transition-group": "^4.4.5",
        "redux-auth-wrapper": "^3.0.0",
        "redux-form": "^8.3.9",
        "redux-thunk": "^2.4.2",
        "roboto-fontface": "^0.10.0",
        "typeface-spectral": "^1.1.13"
    },
    "types": "./src/types.d.tsx"
}
